AA,KAdA;AAeA,MAAA,GAAA,EAAA;AAfA,KAAA;AAiBA,GAvBA;AAwBA,EAAA,OAxBA,qBAwBA;AAAA;;AACA,IAAA,aAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,IAAA,EAAA;AACA,KAFA;AAGA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,kBAAA,EAAA,4BAAA,QAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KALA;AAOA,IAAA,mBAPA,iCAOA;AAAA;;AACA,UAAA,KAAA,GAAA;AACA,QAAA,MAAA,EAAA,KAAA,MADA;AAEA,QAAA,QAAA,EAAA,KAAA;AAFA,OAAA;AAIA,UAAA,OAAA,GAAA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAAA,YAAA,CAAA,KAAA,WAAA,CAAA,OAAA,CAAA,KAAA,OAAA,CAAA,CAFA;AAGA,QAAA,YAAA,EAAA,KAAA,YAHA;AAIA,QAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAAA,IAAA,CAJA;AAKA,QAAA,KAAA,EAAA;AALA,OAAA;AAOA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,MAAA,aAAA,CAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AAEA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAGA,SAPA,MAOA;AACA,UAAA,MAAA,CAAA,cAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,GAAA,GAAA,GAAA,CAAA,GAAA;AACA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,cAAA,GAAA,KAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAGA;AACA,OAhBA;AAiBA;AArCA;AA7BA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- alert -->\n    <v-alert\n      class=\"alert\"\n      outlined\n      type=\"success\"\n      text\n      v-show=\"showSuccessDialog\"\n      transition=\"scroll-y-transition\"\n    >\n      优惠发放成功！\n    </v-alert>\n    <!-- alert -->\n    <v-alert\n      class=\"alert\"\n      outlined\n      type=\"warning\"\n      text\n      v-show=\"showFailDialog\"\n      transition=\"scroll-y-transition\"\n    >\n      {{ msg }}\n    </v-alert>\n\n    <v-container class=\"ma-8 pa-4\">\n      <v-row>\n        <router-link :to=\"`/admin/coupon/create`\">\n          <v-btn class=\"ma-4 mb-8\" fab dark color=\"indigo\">\n            <v-icon dark large>\n              mdi-plus\n            </v-icon>\n          </v-btn>\n        </router-link>\n      </v-row>\n      <v-row>\n        <coupon-item\n          v-for=\"c in couponList\"\n          :key=\"c.id\"\n          :id=\"c.id\"\n          :type=\"c.type\"\n          :scope=\"c.scope\"\n          :name=\"c.name\"\n          :description=\"c.description\"\n          :metadata=\"c.metadata\"\n          :startTime=\"c.startTime\"\n          :endTime=\"c.endTime\"\n          :vaild=\"c.vaild\"\n          :sharable=\"c.sharable\"\n          @deliver-coupon=\"handleDeliverClick\"\n        >\n        </coupon-item>\n      </v-row>\n    </v-container>\n\n    <!-- 分发对话框 -->\n    <v-dialog v-model=\"showDeliverDialog\" width=\"500\">\n      <form class=\"pa-12 grey lighten-5 mt-8\">\n        <v-select\n          :items=\"deliverList\"\n          v-model=\"deliver\"\n          label=\"分发方式\"\n        ></v-select>\n        <v-text-field\n          v-show=\"deliver === 'SINGLE'\"\n          v-model=\"userId\"\n          label=\"用户ID\"\n        ></v-text-field>\n        <v-text-field\n          v-show=\"deliver === 'COURSE_LIKE'\"\n          v-model=\"userId\"\n          label=\"课程ID\"\n        ></v-text-field>\n        <v-switch\n          v-model=\"canDuplicate\"\n          :label=\"\n            `是否可以发放给已经收到过该优惠券的用户: ${canDuplicate.toString()}`\n          \"\n        ></v-switch>\n        <v-text-field v-model=\"nums\" label=\"发放数量\"></v-text-field>\n      </form>\n      <v-btn class=\"ma-0 info\" @click=\"handleDeliverCoupon\">\n        确认\n      </v-btn>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport CouponItem from \"@/components/CouponItem.vue\";\nimport { getCouponList, deliverCoupon } from \"@/api/coupon\";\n\nexport default {\n  name: \"CouponList\",\n  components: {\n    CouponItem\n  },\n  data() {\n    return {\n      couponList: [],\n\n      couponId: \"\",\n      deliverList_: [\"SINGLE\", \"COURSE_LIKE\", \"ALL\"],\n      deliverList: [\"指定单一用户\", \"为某课程点赞的用户\", \"所有用户\"],\n      deliver: \"SINGLE\",\n      userId: \"\",\n      courseId: \"\",\n      canDuplicate: false,\n      nums: 1,\n\n      showDeliverDialog: false,\n      showSuccessDialog: false,\n      showFailDialog: false,\n      msg: \"\"\n    };\n  },\n  mounted() {\n    getCouponList().then(res => {\n      this.couponList = res.data || [];\n    });\n  },\n  methods: {\n    handleDeliverClick: function(couponId) {\n      console.log(\"****\", couponId);\n      this.couponId = couponId;\n      this.showDeliverDialog = true;\n    },\n\n    handleDeliverCoupon() {\n      const extra = {\n        userId: this.userId,\n        courseId: this.courseId\n      };\n      const payload = {\n        couponId: this.couponId,\n        deliver: this.deliverList_[this.deliverList.indexOf(this.deliver)],\n        canDuplicate: this.canDuplicate,\n        nums: Number.parseInt(this.nums),\n        extra: extra\n      };\n      console.log(payload);\n      deliverCoupon(payload).then(res => {\n        console.log(res);\n        if (res.code === 1) {\n          this.showSuccessDialog = true;\n          this.showDeliverDialog = false;\n\n          setTimeout(() => {\n            this.showSuccessDialog = false;\n          }, 1000);\n        } else {\n          this.showFailDialog = true;\n          this.msg = res.msg;\n          setTimeout(() => {\n            this.showFailDialog = false;\n          }, 1000);\n        }\n      });\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/admin"}]}